pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function init_s(o)
	o.s = 0
	o.x = 0
	o.y = 0
	o.flip = {}
	o.flip.x = false
	o.flip.y = false
end

function draw_s(o) 
	if o.s ~= nil then
		spr(o.s, o.x, o.y, 1, 1, o.flip.x, o.flip.y)
	end
end

player = {}

os = {}

function _init()
	init_s(player)
	player.s = 1
	add(os, player)
end

function _draw()
	cls(14)
	for _, o in pairs(os) do
		draw_s(o)
	end
end

